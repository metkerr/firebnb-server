<div
  class="tab-pane fade show active"
  id="nav-show-item"
  role="tabpanel"
  aria-labelledby="nav-show-item-tab"
>
  <div class="row">
    <% for(image of item.imageId){ %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="https://firebnb-sysadmin.herokuapp.com/<%= image.imageUrl %>"
            alt="item image"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a href="/admin/item" class="btn btn-warning">Back</a>
</div>
